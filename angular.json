{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "client-application": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/client-application",
      "sourceRoot": "projects/client-application/src",
      "prefix": "app",
      "i18n": {
        "sourceLocale": {
          "code": "fr",
          "baseHref": ""
        },
        "locales": {
          "en": {
            "baseHref": "/en/",
            "translation": "projects/client-application/src/locale/messages.en.xlf"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/client-application",
            "index": "projects/client-application/src/index.html",
            "main": "projects/client-application/src/main.ts",
            "polyfills": "projects/client-application/src/polyfills.ts",
            "tsConfig": "projects/client-application/tsconfig.app.json",
            "localize": true,
            "allowedCommonJsDependencies": ["lodash", "leaflet.locatecontrol"],
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/client-application/src/favicon.ico",
              "projects/client-application/src/assets",
              "projects/client-application/src/sitemap.xml",
              "projects/client-application/src/robots.txt",
              "projects/client-application/src/ngsw-worker.js",
              "projects/client-application/src/ngsw.json",
              "projects/client-application/src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input": "./node_modules/leaflet/dist/images",
                "output": "assets/"
              }
            ],
            "styles": [
              "projects/client-application/src/styles.scss",
              "./node_modules/leaflet/dist/leaflet.css",
              "./node_modules/leaflet.locatecontrol/dist/L.Control.Locate.css"
            ],
            "scripts": [],
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json",
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "15kb",
                  "maximumError": "20kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/client-application/src/environments/environment.ts",
                  "with": "projects/client-application/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json",
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "fr": {
              "localize": ["fr"]
            },
            "en": {
              "localize": ["en"]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "client-application:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "client-application:build:production"
            },
            "development": {
              "browserTarget": "client-application:build:development"
            },
            "fr": {
              "browserTarget": "client-application:build:fr"
            },
            "en": {
              "browserTarget": "client-application:build:en"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client-application:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "configPath": "jest.config.js",
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": ["scss"],
            "assets": ["src/favicon.ico", "projects/client-application/src/assets"],
            "styles": ["src/main.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/client-application/src/**/*.ts", "projects/client-application/src/**/*.html"]
          }
        }
      }
    },
    "@gouvfr-anct/mediation-numerique": {
      "projectType": "library",
      "root": "projects/@gouvfr-anct/mediation-numerique",
      "sourceRoot": "projects/@gouvfr-anct/mediation-numerique/src",
      "prefix": "fr",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/@gouvfr-anct/mediation-numerique/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/@gouvfr-anct/mediation-numerique/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/@gouvfr-anct/mediation-numerique/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "configPath": "jest.config.js",
            "tsConfig": "tsconfig.spec.json",
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/@gouvfr-anct/mediation-numerique/src/**/*.ts",
              "projects/@gouvfr-anct/mediation-numerique/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "client-application",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
